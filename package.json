{
  "name": "@mcpsq/mcpsq",
  "version": "1.1.3",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "mcpsq": "build/index.js"
  },
  "files": [
    "build/**/*",
    "data/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xinbenlv/mcpsq.git"
  },
  "homepage": "https://mcpsq.xyz",
  "bugs": {
    "url": "https://github.com/xinbenlv/mcpsq/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon",
    "build": "tsc",
    "clean": "rm -rf build",
    "validate:data": "tsx src/validate-data.ts",
    "typecheck": "tsc --noEmit",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [
    "mcp",
    "modelcontextprotocol",
    "cursor",
    "cursor-mcp"
  ],
  "author": "Zainan Victor Zhou <zzn-github@zzn.im>",
  "license": "MIT",
  "description": "A simple MCP server to discover and manage MCP servers of your needs",
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/node": "^22.10.0",
    "nodemon": "^3.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.0",
    "express": "^5.1.0",
    "zod": "^3.23.8"
  }
}
